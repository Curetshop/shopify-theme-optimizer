ainer {\n                padding: 10px 5px;\n                width: 100vw;\n                position: relative;\n                left: 50%;\n                right: 50%;\n                margin-left: -50vw;\n                margin-right: -50vw;\n                margin-top: -8px;\n            }\n            \n            .time-value {\n                font-size: 1.2em;\n                padding: 6px 12px;\n                min-width: 45px;\n            }\n            \n            .time-label {\n                font-size: 0.55em;\n            }\n            \n            .separator {\n                font-size: 1em;\n                margin: 0 1px;\n            }\n            \n            .countdown-display {\n                gap: 3px;\n            }\n            \n            .time-unit {\n                min-width: 55px;\n            }\n        }\n    <\/style>\n<\/head>\n<body>\n    <div class=\"countdown-container\">\n        <div class=\"countdown-display\" id=\"countdown\">\n            <div class=\"time-unit\">\n                <div class=\"time-value\" id=\"days\">00<\/div>\n                <div class=\"time-label\">Días<\/div>\n            <\/div>\n            <div class=\"separator\">:<\/div>\n            <div class=\"time-unit\">\n                <div class=\"time-value\" id=\"hours\">00<\/div>\n                <div class=\"time-label\">Horas<\/div>\n            <\/div>\n            <div class=\"separator\">:<\/div>\n            <div class=\"time-unit\">\n                <div class=\"time-value\" id=\"minutes\">00<\/div>\n                <div class=\"time-label\">Minutos<\/div>\n            <\/div>\n            <div class=\"separator\">:<\/div>\n            <div class=\"time-unit\">\n                <div class=\"time-value\" id=\"seconds\">00<\/div>\n                <div class=\"time-label\">Segundos<\/div>\n            <\/div>\n        <\/div>\n\n        <div class=\"expired-message\" id=\"expiredMessage\">\n            🎉 ¡La oferta ha terminado! 🎉\n        